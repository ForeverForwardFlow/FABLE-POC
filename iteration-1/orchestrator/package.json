{
  "name": "@fable/orchestrator",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "FABLE orchestrator - plans and coordinates worker tasks",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "demo": "npx tsx src/demo.ts",
    "demo:plan": "npx tsx src/demo.ts --plan",
    "demo:loop": "npx tsx src/demo.ts --loop",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@fable/shared": "*",
    "@modelcontextprotocol/sdk": "^1.25.3",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.7",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  }
}
